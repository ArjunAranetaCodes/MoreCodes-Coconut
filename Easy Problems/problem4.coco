
PI = 3.14
dia = 0.0
area = 0.0
cir = 0.0

radius = input("Enter value of radius: ")
radius = float(radius)

dia = radius * radius
area = PI * dia
cir = 2 * PI * radius

"Diameter of the circle is " + str(dia) |> print
"Area of the circle is " + str(area) |> print
"Circumference of the circle is " + str(cir) |> print


